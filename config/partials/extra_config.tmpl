{
   "documentation/openapi": {
      "version": "1",
      "contact_name": "John Doe",
      "contact_email": "contact@example.com",
      "license_name": "MIT",
      "license_url": "https://opensource.org/licenses/MIT"
   },
   "router":{
      "return_error_msg":true,
      "forwarded_by_client_ip":true,
      "disable_access_log":true
   },
   "telemetry/logging":{
      "level":"INFO",
      "prefix":"[SIMFINY API-GATEWAY]",
      "syslog":false,
      "stdout":true
   },
   "telemetry/opencensus":{
      "exporters":{
         "extra_config":{
            "newrelic":{
               "service_name":"simfiny-api-gateway",
               "api_key":"62fd721c712d5863a4e75b8f547b7c1ea884NRAL",
               "timeout":"15s",
               "spans_url":"https://trace-api.newrelic.com/trace/v1",
               "metrics_url":"https://metric-api.newrelic.com/metric/v1",
               "debug":false
            }
         }
      }
   },
   "telemetry/metrics":{
      "collection_time":"60s",
      "proxy_disabled":false,
      "router_disabled":false,
      "backend_disabled":false,
      "endpoint_disabled":false,
      "listen_address":":8081"
   },
   "security/cors":{
      "allow_origins":[
         "*"
      ],
      "allow_methods":[
         "GET",
         "HEAD",
         "POST",
         "PUT",
         "PATCH",
         "DELETE",
         "OPTIONS"
      ],
      "allow_headers":[
         "*"
      ],
    "expose_headers": [
        "Content-Length"
      ],
      "max_age":"12h"
   }
}